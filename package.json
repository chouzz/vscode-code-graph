{
  "name": "vscode-code-graph",
  "displayName": "vscode-code-graph",
  "description": "",
  "version": "0.0.1",
  "publisher": "chouzz",
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Chouzz",
    "url": "https://github.com/chouzz"
  },
  "galleryBanner": {
    "color": "#4897f7",
    "theme": "light"
  },
  "bugs": {
    "url": "https://github.com/chouzz/vscode-code-graph/issues",
    "email": "zhouhua25@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chouzz/vscode-code-graph.git"
  },
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onView:codeGraph.view"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-code-graph.helloWorld",
        "title": "Hello World"
      }
    ],
    "views": {
      "codeGraphPanel": [
        {
          "type": "webview",
          "id": "codeGraph.view",
          "name": "Code Graph",
          "icon": "$(references)",
          "visibility": "hidden"
        }
      ]
    },
    "viewsContainers": {
      "panel": [
        {
          "id": "codeGraphPanel",
          "title": "Code Graph",
          "icon": "$(references)"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.10",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "18.x",
    "@types/vscode": "^1.73.1",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.47.1",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.31.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
